{
  "tags": {
      "allowUnknownTags": true,
      "dictionaries": ["jsdoc","closure"]
  },
  "source": {
      "include": ["src"],
      "includePattern": ".+\\.js(doc|x)?$",
      "excludePattern": "(node_modules/|docs)"
  },
  "plugins": [
      "plugins/markdown",
      "better-docs/component"
  ],
   "templates": {
    "better-docs": {
        "name": "My React Practice components"
      }
  },
  "opts": {
      "destination": "docs",
      "recurse": true,
      "verbose": true,
      "readme": "README.md"
  }
}